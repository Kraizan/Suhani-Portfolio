<section id="wallpaper-carousel">
  <div id="wallpapers" class="carousel slide w-75 mx-auto" data-bs-ride="carousel">
    <div class="carousel-indicators">
      <button type="button" data-bs-target="#wallpapers" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
      <button type="button" data-bs-target="#wallpapers" data-bs-slide-to="1" aria-label="Slide 2"></button>
      <button type="button" data-bs-target="#wallpapers" data-bs-slide-to="2" aria-label="Slide 3"></button>
    </div>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <a href="wallpaper/<%=0%>"><img src="<%=images[0]%>" class="d-block w-10" alt="..."></a>
      </div>
      <div class="carousel-item">
        <a href="wallpaper/<%=1%>"><img src="<%=images[1]%>" class="d-block w-10" alt="..."></a>
      </div>
      <div class="carousel-item">
        <a href="wallpaper/<%=2%>"><img src="<%=images[2]%>" class="d-block w-10" alt="..."></a>
      </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#wallpapers" data-bs-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#wallpapers" data-bs-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
  </div>
</section>
